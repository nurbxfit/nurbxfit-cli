{
  "name": "nurbxfit-cli",
  "version": "1.0.0",
  "description": "A CLI tool for creating projects using starter template from nurbxfit github",
  "bin": {
    "nurbxfit": "./bin/nurbxfit-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q bin/nurbxfit-cli.js\"",
    "start": "node dist/index.js",
    "preview": "node bin/nurbxfit-cli.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "nurbxfit",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "files": [
    "dist/**/*",
    "bin/**/*"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "fs-extra": "^11.3.2"
  }
}